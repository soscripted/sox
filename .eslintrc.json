{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "greasemonkey": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "comma-dangle": ["error", "always-multiline"],
        "no-console":"off",
        "prefer-arrow-callback":"warn",
        "no-trailing-spaces":"error"
    }
}